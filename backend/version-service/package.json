{
  "name": "version-service",
  "version": "1.0.0",
  "description": "Version service with S3 upload and SSE support",
  "main": "index.ts",
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "start": "bun index.ts",
    "dev": "bun --watch index.ts",
